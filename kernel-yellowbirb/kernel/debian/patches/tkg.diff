+static inline struct page *find_or_create_page(struct address_space *mapping,
+					pgoff_t index, gfp_t gfp_mask)
+{
+	return pagecache_get_page(mapping, index,
+					FGP_LOCK|FGP_ACCESSED|FGP_CREAT,
+					gfp_mask);
